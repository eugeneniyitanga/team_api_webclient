  <table>
    <tr>
      <td>Creature</td>
      <td>Name</td>
      <td>State</td>
      <td>Sport</td>
    </tr>
    <% @teams.each do |team| %>
      <tr>
          <td><%= team["creature"] %></td> 
          <td><%= team["name"] %></td> 
          <td><%= team["state"] %></td> 
          <td><%= team["sport"] %></td> 
      </tr>
    <% end %>
  </table> 